{
    "name": "denoby/laravel-telegram-logger",
    "description": "Laravel package for sending logs and messages to Telegram",
    "keywords": ["laravel", "telegram", "logger", "monolog", "logging"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "DenoBY",
            "email": "denis.baidak@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "monolog/monolog": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "DenoBY\\TelegramLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DenoBY\\TelegramLogger\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DenoBY\\TelegramLogger\\ServiceProvider"
            ],
            "aliases": {
                "Telegram": "DenoBY\\TelegramLogger\\Facades\\Telegram"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
